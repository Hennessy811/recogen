{
	"name": "recogen",
	"description": "Generate React components by templates nice and easy",
	"version": "0.1.5",
	"author": "Malakhov Dmitriy",
	"bin": {
		"recogen": "./bin/run"
	},
	"bugs": "https://github.com/Hennessy811/recogen/issues",
	"dependencies": {
		"@oclif/command": "^1",
		"@oclif/config": "^1",
		"@oclif/plugin-help": "^3",
		"@types/fs-extra": "^9.0.1",
		"@types/inquirer": "^7.3.1",
		"cli-ux": "^5.5.0",
		"fs-extra": "^9.0.1",
		"inquirer": "^7.3.3",
		"tslib": "^1"
	},
	"devDependencies": {
		"@oclif/dev-cli": "^1",
		"@oclif/test": "^1",
		"@types/chai": "^4",
		"@types/mocha": "^5",
		"@types/node": "^10",
		"@typescript-eslint/eslint-plugin": "^4.4.0",
		"@typescript-eslint/parser": "^4.4.0",
		"chai": "^4",
		"eslint": "^7.10.0",
		"eslint-config-oclif": "^3.1",
		"eslint-config-oclif-typescript": "^0.1",
		"eslint-config-prettier": "^6.12.0",
		"eslint-plugin-prettier": "^3.1.4",
		"globby": "^10",
		"mocha": "^5",
		"nyc": "^14",
		"prettier": "^2.1.2",
		"ts-node": "^8",
		"typescript": "^3.3"
	},
	"engines": {
		"node": ">=8.0.0"
	},
	"files": [
		"/bin",
		"/lib",
		"/npm-shrinkwrap.json",
		"/oclif.manifest.json"
	],
	"homepage": "https://github.com/Hennessy811/recogen",
	"keywords": [
		"oclif"
	],
	"license": "MIT",
	"main": "lib/index.js",
	"oclif": {
		"commands": "./lib/commands",
		"bin": "recogen",
		"plugins": [
			"@oclif/plugin-help"
		]
	},
	"repository": "Hennessy811/recogen",
	"scripts": {
		"postpack": "rm -f oclif.manifest.json",
		"posttest": "eslint . --ext .ts --config .eslintrc",
		"prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
		"test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
		"version": "oclif-dev readme && git add README.md",
		"lint": "eslint '*/**/*.ts' --fix"
	},
	"types": "lib/index.d.ts"
}
